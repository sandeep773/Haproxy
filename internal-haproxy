global
  log 127.0.0.1 local0
  maxconn 100000
  ulimit-n 4000033
  maxpipes 400000
  nbthread 3
  cpu-map auto:1/1-3   1-3
  user haproxy
  group root
  tune.ssl.default-dh-param 2048
  stats socket /var/run/haproxy/haproxy.sock level admin mode 600
  stats timeout 30s
  server-state-file server-state-sync
  server-state-base /var/run/haproxy/
  daemon

defaults
  log   global
  mode  http
  option  httplog
  option  http-ignore-probes
  retries 3
  option redispatch
  timeout http-request 120s
  timeout client-fin  5s
  timeout connect 3s
  timeout client  300s
  timeout server  300s
  option splice-auto
  option tcp-smart-connect
  option tcp-smart-accept
  maxconn 400000

  # %ci  | client_ip                 (accepted address)                 | IP
  # %cp  | client_port               (accepted address)                 | numeric
  # %tr  | date_time of HTTP request                                    | date
  # %f   | frontend_name_transport                                      | string
  # %b   | backend_name                                                 | string
  # %s   | server_name                                                  | string
  # %TR  | time to receive the full request from 1st byte               | numeric
  # %Tw  | total time spent in the queues waiting for a connection slot | numeric
  # %Tc  | total time to establish the TCP connection to the server     | numeric
  # %Tr  | Tr (response time)                                           | numeric
  # %Ta  | Active time of the request (from TR to end)                  | numeric
  # %ST  | status_code                                                  | numeric
  # %B   | bytes_read           (from server to client)                 | numeric
  # %ac  | actconn                                                      | numeric
  # %fc  | feconn     (frontend concurrent connections)                 | numeric
  # %bc  | beconn      (backend concurrent connections)                 | numeric
  # %sc  | srv_conn     (server concurrent connections)                 | numeric
  # %ts  | termination_state                                            | string
  # %sq  | srv_queue                                                    | numeric
  # %bq  | backend_queue                                                | numeric
  # %r   | http_request                                                 | string
  # %hrl | captured_request_headers CLF style                           | string list
  log-format "hap-logs,%ci:%cp,%tr,%f,%b,%s,%TR,%Tw,%Tc,%Tr,%Ta,%ST,%B,%ac,%fc,%bc,%sc,%ts,%sq,%bq,%{+Q}r,%hr"

  errorfile 400 /etc/haproxy/errors/400.http
  errorfile 403 /etc/haproxy/errors/403.http
  errorfile 408 /etc/haproxy/errors/408.http
  errorfile 500 /etc/haproxy/errors/500.http
  errorfile 502 /etc/haproxy/errors/502.http
  errorfile 503 /etc/haproxy/errors/503.http
  errorfile 504 /etc/haproxy/errors/504.http

listen stats-prometheus
  bind *:8090
  mode http
  stats enable
  stats uri /status

resolvers dns-private
  nameserver public-1 172.31.0.2:53
  accepted_payload_size 8192
  hold valid 1s

resolvers dns-1
  nameserver public-1  8.8.8.8:53
  hold valid 1s

frontend app-internal-http
  bind *:9000

  option httpclose
  # Capture the Rate Limit Headers and Actual Client IP in Logs
  capture request header Host len 32
  capture request header X-Forwarded-For len 64
  capture request header User-Agent len 256
  capture request header cf-ray len 40
  capture request header cf-request-id len 64
  capture request header X-Haproxy-ACL len 256
  capture request header X-Bad-User len 64
  capture request header X-City len 64
  capture request header tid len 40
  capture request header token len 80
  capture request header deviceId len 48
  capture request header swuid len 40
  capture request header X-haproxy-crs len 256

  acl http req.hdr(X-Forwarded-Proto) http
  redirect scheme https if http

  acl allow_from_everywhere hdr(X-Forwarded-For) 0.0.0.0/0
  acl allow_from_everywhere src 0.0.0.0/0
  acl swiggy_pan_india_ips req.hdr_ip(X-Forwarded-For) -f /etc/haproxy/security/swiggy_pan_india_ips.lst
  acl dp-ingest_ips hdr(X-Forwarded-For) -f /etc/haproxy/security/dp-ingest-ip.lst
  acl swiggy_hq_ips req.hdr_ip(X-Forwarded-For) -f /etc/haproxy/security/vpn_ip.lst -f /etc/haproxy/security/nmdc_ip.lst
  acl swiggy_vpn_ips req.hdr_ip(X-Forwarded-For) -f /etc/haproxy/security/vpn_ip.lst
  acl gamooga_ips hdr(X-forwarded-For) -f /etc/haproxy/security/gamooga.lst
  acl jenkins_ip hdr(X-forwarded-For) -f /etc/haproxy/security/jenkins.lst
  acl github_ip hdr(X-forwarded-For) -f /etc/haproxy/security/github_ips.lst
  acl jenkins_ci_ip hdr(X-forwarded-For) -f /etc/haproxy/security/jenkin_ci_ips.lst
  acl whitelist_ips hdr(X-Forwarded-For) -f /etc/haproxy/security/int-whitelist.lst
  acl github_ip src 192.30.252.0/22 185.199.108.0/22 192.30.252.0/22 185.199.108.0/22 140.82.112.0/20 140.82.112.0/20
  acl github_ip hdr(X-Forwarded-For) 192.30.252.0/22 185.199.108.0/22 192.30.252.0/22 185.199.108.0/22 140.82.112.0/20 140.82.112.0/20
  acl ads-external_ip hdr(X-Forwarded-For) -f /etc/haproxy/security/ads-external_ip.lst
  acl timeline_ip hdr(X-forwarded-For) -f /etc/haproxy/security/timeline_ip.lst
  acl netscope_ip req.hdr_ip(X-forwarded-For) -f /etc/haproxy/security/netscope_ips.lst
  acl shared_nexus_ip req.hdr_ip(X-forwarded-For) -f /etc/haproxy/security/shared_nexus.lst
  acl cms-sandbox_ip req.hdr_ip(X-forwarded-For) -f /etc/haproxy/security/cms-sandbox.lst
  # CMR-33198 for ci.swiggy.co
  acl ci_swiggy_co req.hdr_ip(X-forwarded-For) 104.192.143.0/24 52.221.63.6/32 52.76.109.42/32 18.139.7.115/32 54.169.125.40/32 3.0.185.66/32 111.93.189.238/32 182.75.181.110/32 182.71.244.18/32

